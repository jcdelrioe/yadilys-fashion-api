{
  "name": "yadilys-fashion-api",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "DB_USER": "@yadilys_db_user",
    "DB_PASSWORD": "yadilys_db_password",
    "DB_HOST": "yadilys_db_host",
    "DB_NAME": "@yadilys_db_name"
  }
}
